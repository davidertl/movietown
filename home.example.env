# Example environment variables for home-compose.yaml

# General settings
# Time zone for the containers
TIMEZONE=Europe/Berlin
# User ID to run services
PUID=1000
# Group ID to run services
PGID=1000
# Default file creation mask
UMASK=002
# Base directory for container data
FOLDER_FOR_DATA=/srv/movietown/data
# Location of your media files
FOLDER_FOR_MEDIA=/mnt/media
# Theme for the landing page
TP_THEME=organizr-dark

# Networks
# Subnet for Docker network
DOCKER_SUBNET=172.23.0.0/16
# Gateway for Docker network
DOCKER_GATEWAY=172.23.0.1
# Subnet for internal services
INTERNAL_SUBNET=172.24.0.0/16
# Gateway for internal services
INTERNAL_GATEWAY=172.24.0.1
# Local LAN range
LOCAL_SUBNET=192.168.0.0/16

# Ports
# Port for PostgreSQL
POSTGRESQL_PORT=5432
# Port for Valkey
VALKEY_PORT=6379
# Control port for Gluetun
GLUETUN_CONTROL_PORT=8000
# Port for FlareSolverr
FLARESOLVERR_PORT=8191
# Tdarr server port
TDARR_SERVER_PORT=8266
# qBittorrent listening port
QBIT_PORT=6881
# Web UI port for qBittorrent
WEBUI_PORT_QBITTORRENT=8200
# Web UI port for Bazarr
WEBUI_PORT_BAZARR=6767
# Web UI port for FileBot
WEBUI_PORT_FILEBOT=5454
# Web UI port for Jellyfin
WEBUI_PORT_JELLYFIN=8096
# Web UI port for Jellyseerr
WEBUI_PORT_JELLYSEERR=5055
# Web UI port for Lidarr
WEBUI_PORT_LIDARR=8686
# Web UI port for Mylar
WEBUI_PORT_MYLAR=8090
# Web UI port for Plex
WEBUI_PORT_PLEX=32400
# Web UI port for Prowlarr
WEBUI_PORT_PROWLARR=9696
# Web UI port for Radarr
WEBUI_PORT_RADARR=7878
# Web UI port for Readarr
WEBUI_PORT_READARR=8787
# Web UI port for Sabnzbd
WEBUI_PORT_SABNZBD=8080
# Web UI port for Sonarr
WEBUI_PORT_SONARR=8989
# Web UI port for Tdarr
WEBUI_PORT_TDARR=8265

# Authentik / database
# Version tag for Authentik
AUTHENTIK_VERSION=latest
# Secret key for Authentik
AUTHENTIK_SECRET_KEY=change-me-very-secret
# Name of Authentik database
AUTHENTIK_DATABASE=authentik
# Username for Authentik's PostgreSQL
POSTGRESQL_USERNAME=authentik
# Password for Authentik's PostgreSQL
POSTGRESQL_PASSWORD=change-me-postgres
# Enable or disable Authentik error reporting
AUTHENTIK_ERROR_REPORTING__ENABLED=false

# Tailscale
# Authentication key for Tailscale
TAILSCALE_AUTH_KEY=tskey-xxxxxxxxxxxxxxxx

# VPN / Gluetun
# Provider name (use 'custom' for WireGuard config)
VPN_SERVICE_PROVIDER=custom
# VPN username credential
VPN_USERNAME=your_vpn_username
# VPN password credential
VPN_PASSWORD=your_vpn_password
# Endpoint IP for VPN
VPN_ENDPOINT_IP=
# Endpoint port for VPN
VPN_ENDPOINT_PORT=
# Type of VPN (wireguard/openvpn)
VPN_TYPE=
# WireGuard public key
WIREGUARD_PUBLIC_KEY=
# WireGuard private key
WIREGUARD_PRIVATE_KEY=
# WireGuard preshared key
WIREGUARD_PRESHARED_KEY=
# WireGuard assigned addresses
WIREGUARD_ADDRESSES=
# Path to custom OpenVPN config
OPENVPN_CUSTOM_CONFIG=
# Limit servers by category
SERVER_CATEGORIES=
# Limit servers by country
SERVER_COUNTRIES=
# Limit servers by city
SERVER_CITIES=
# Limit servers by region
SERVER_REGIONS=
# Limit servers by hostname
SERVER_HOSTNAMES=
